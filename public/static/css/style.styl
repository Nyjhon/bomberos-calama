color1 = #2caaff
color2 = #fdd700
color3 = #334F9A
body
	background-color  #D1CDEB
	font-size 14px
	font-family 'Roboto', sans-serif;
	
a
.btn
	&:active
	&:focus
		box-shadow none
		outline none
.mtop32
	margin-top 32px
.mbot32
	margin-bottom 32px
.mtop16
	margin-top 16px
.mtop8
	padding-top 8px
.form-control
	&:hover
	&:focus
		box-shadow none
nav
	background-color color1
	border-bottom 2px solid color1
	.navbar-brand
		img
			width 80px
			margin-bottom -15px
			margin-top -12px
	.navbar-toggler
		color color1
		&:active
		&:focus
			box-shadow none
			outline none
	.navbar-nav
		li.nav-item
			a.nav-link
				border 1px solid #fff
				border-radius 4px
				color #000
				font-size 1em
				font-weight bold
				margin-right 4px
				padding 5px 16px !important
				&:hover
					color #fff
			a.btn
				border 1px solid #000
				color #000
				&:hover
					background-color color2
			ul.dropdown-menu
				border-radius 0px 0px 4px 4px
				background-color #fff
				border 0px
				top 150% !important
				li
					border-bottom 1px solid #fff
					a
						color #000
						display block
						float none
						width 100%
						i
							color color3
						&:last-child
							border-bottom 0px
						&:hover
							background-color #ACACAC !important
		li.link-acc
			overflow hidden
			a
				display inline-block
				float left
				margin-right 0px !important
				&:first-child
					border-radius 4px 0px 0px 4px
					border-right 1px solid #fff
				&:last-child
					border-radius 0px 4px 4px 0px
				img 
					position relative
					top -2px
					margin-right 6px
					width 24px
		li.link-user
			overflow initial
			a
				border 0px !important
				img
					border-radius 50%
				&:hover
					background-color transparent !important
.navbar-expand-lg
	position fixed
	width 100%
	z-index 2
.wrapper
	position relative
	margin-top 0px
	padding-top 90px
	item-aling center
	.panel
		background-color #feffd4
		border-radius 6px
		border-bottom 2px solid #2caaff
		width 100%
		.header
			border-bottom 1px solid #e8e8e8
			h2.title
				display inline-block
				color #19213d
				font-size 1.1em
				margin-bottom 0px
				padding 12px 16px
			ul
				display inline-block
				float right
				margin-right 12px
				li
					display inline-block
					a
						display inline-block
						padding 10px
						color #333
						&:hover
							background-color #f6f6f6
							text-decoration none
							color #222
						i
							color #2caaff
					ul
						background-color #fff
						border 1px solid #f0f0f0
						display none
						float none
						position absolute
						width 160px
						li
							display inline-block
							border-bottom 1px solid #f0f0f0
							width 100%
							&:last-child
								border-bottom 0px
							a
								display block
					&:hover
						ul
							display block
		.inside
			padding 16px
			.nav
				.nav-link
					border 1px solid black
					margin-right 8px
					&:hover
						background-color #2caaff
						color #fff
			.form_buscar
				display none
				margin-bottom 16px
				.btn
					width 100%
			.big_count
				display block
				text-align center
				width 100%
				font-size 2em			
			.edit_avatar
				a
					display block
					margin 0px auto
					width 140px
					position relative
					.overlay
						background-color rgba(0, 0, 0, .55)
						border-radius 50%
						color #fff
						font-size 2em
						display none
						align-items center
						height 140px
						width 100%
						justify-content center
						position absolute
						img
							width 60px
					&:hover
						.overlay
							display flex
				img
					border-radius 50%
					display block
					width 100%
				.form-control
					display none
			.btn-primary
				background-color color1
				border 0px
				padding 8px 32px !important
				&:hover
					background-color color2
					color #000
.mdslider
	display flex 
	flex-wrap wrap
	margin-top 0px
	min-height 380px
	overflow hidden
	position relative
	width 100%
	.md-slider-item
		align-items center
		border-right 1px solid #ffff
		border-radius 5px 5px 5px 5px
		display none
		background-color color1 
		color #fff
		font-size 4em
		justify-content center
		position absolute
		min-height 380px
		transition .3s all ease-in-out
		width 100% !important
		.col-md-8
			padding-right 0px
		.col-md-4
			padding-left 0px
		.content
			font-family: 'Roboto', sans-serif;
			background-color #fff
			border-radius 5px 5px 5px 5px
			align-items center
			display flex 
			justify-content center
			height calc(380px - 32px)
			margin 16px 16px 16px 16px
			border 3px solid #000
			h1
				font-family: 'Roboto', sans-serif;
				display block
				text-align center
				width 100%
				font-size .5em
				font-weight bold
				color #000
			h2
				display block
				text-align center
				width 100%	
				font-size .4em
				color #283747
	ul.navigation
		display block
		margin 0px
		padding 0px
		position absolute
		overflow hidden
		top calc(50% - 18px)
		z-index 1
		width 100%
		li
			display inline-flex
			float left
			a
				align-items center
				background-color rgba(0, 0, 0, .5)
				border-radius 50%
				display flex
				justify-content center
				font-size 1.4em
				height 36px
				width 36px
				text-decoration none
				color #fff
			&:first-child
				margin-left 32px
				a
					i
						position relative
						margin-left -2px
						margin-top 2px
			&:last-child
				margin-right 32px
				float right
				a
					i
						position relative
						margin-left 2px
						margin-top 2px
.cuadrado-perfecto		
	width 100%
	padding-bottom 100%
	position relative
	img 
		width 100%
		height 380px
		object-position center
		top 0px
		left 0px
		border-radius 5px 0px 0px 5px
.container
	padding 0px
	.row
		.col-md-5
			padding-right 0px
		.col-md-7
			padding-left 0px
			.inf
				height 300px
				padding-left 16px
				text-align center
				.telefonos
					h5
						text-align right
					.tel
						text-align left
.col-md-6
	margin-top 16px
	text-align center
	h2
		color red
	p
		padding 8px 32px 8px 32px
		font-size 1.3em
		color #100170
.base
	color #fff
	height 100px
	background-color #000
	margin-left 0px
	margin-right 0px
	border-bottom 3px solid color3
	border-radius 0px 0px 5px 5px
	.sociales
		padding-top 10px
		text-align right
	a
		color #fff
		margin 0 7.5px
		text-decoration none
		i
			font-size 30px
			color #fff
	p
		text-align center
		font-size 1.8em
@media only screen and (max-width 720px)
	nav
		.navbar-nav
			display block !important
			margin-top 5px
			overflow hidden
			li.nav-item
				display block
				float left
				margin-right 0px
				margin-bottom 8px
				width 50%
				a.nav-link
					text-align center
					color color3
					span
						display block
						font-size 1em
						font-weight normal
						margin-top 4px
						width 100%
					i
						font-size 3em
				&:last-child
					float inline-end
					width 100%
			li.link-acc
				a
					font-size 1.2em
					width 50% !important
					i
						font-size 1em !important
			li.link-user
				border 1px solid #d6d6d6 !important
				border-radius 6px
				padding 8px
				a
					display block
					font-size 1.2em
					width 100% !important
					margin-top 12px
					img
						width 36px
				ul.dropdown-menu
					top 0px !important
					hr.dropdown-divider
						display none
				ul.shadow
					box-shadow none !important
				 